<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/DesignationCategoryDef[defName="Structure"]/specialDesignatorClasses</xpath>
		<value>
			<li>Replace_Stuff.Designator_ReplaceStuff</li>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName="Wall"]/building/relatedBuildCommands</xpath>
		<value>
			<li>Cooler_Over</li>
			<li>Cooler_Over2W</li>
			<li>Vent_Over</li>
		</value>
	</Operation>

	<Operation Class="PatchOperationConditional">
		<xpath>Defs/ThingDef[defName="Door"]/building/relatedBuildCommands</xpath>
		<nomatch Class="PatchOperationAdd">
		  <xpath>Defs/ThingDef[defName="Door"]/building</xpath>
		  <value>
			<relatedBuildCommands />
		  </value>
		</nomatch>
	  </Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[defName="Door"]/building/relatedBuildCommands</xpath>
		<value>
			<li>Autodoor</li>
		</value>
	</Operation>
</Patch>